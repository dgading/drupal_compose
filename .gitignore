# Ignore directories generated by Composer
/drupal/drush/contrib/
/drupal/vendor/
/drupal/web/core/
/drupal/web/modules/contrib/
/drupal/web/themes/contrib/
/drupal/web/profiles/contrib/
/drupal/web/libraries/

# Ignore sensitive information
/drupal/web/sites/*/settings.php
/drupal/web/sites/*/settings.local.php

# Ignore Drupal's file directory
/drupal/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/drupal/web/sites/simpletest

# Ignore actual data in data directories
/data/db/*
/data/files/*
!/data/db/.gitignore
!/data/files/.gitignore

# Ignore files generated by PhpStorm
/drupal/.idea/
*.DS_Store*